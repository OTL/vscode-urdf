{
    "name": "urdf",
    "displayName": "URDF",
    "description": "Support of URDF/xacro in VSCode",
    "version": "0.0.1",
    "publisher": "smilerobotics",
    "engines": {
        "vscode": "^1.18.0"
    },
    "extensionDependencies": [
        "xml"
    ],
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "xml",
                "aliases": [
                    "urdf",
                    "URDF"
                ],
                "extensions": [
                    ".urdf",
                    ".xacro"
                ]
            }
        ],
        "snippets": [
            {
                "language": "xml",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "devDependencies": {
        "vscode": "^1.1.6"
    }
}